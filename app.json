{
  "expo": {
    "name": "Wally",
    "slug": "wally",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wally_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wally_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.urboyrip.wally"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/wally_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/wally_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "677ec0a7-a6f8-4c78-afe2-79f749f65dd1"
      }
    }
  }
}
